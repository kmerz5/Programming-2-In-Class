<Window x:Class="_P__GOT_Quote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_P__GOT_Quote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://i.ytimg.com/vi/jgZXDeGjf84/maxresdefault.jpg"/>
        </Grid.Background>
        <Button x:Name="btn_getquote" Content="Get Quote!" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" Height="53" Width="122" Background="#FF6D92AC" FontWeight="Bold" FontSize="20" BorderBrush="#FF120909" Click="btn_getquote_Click"/>
        <TextBlock x:Name="txt_quote" HorizontalAlignment="Left" Margin="498,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="114" Width="292" Background="#FF6D92AC" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="btn_export" Content="Export" HorizontalAlignment="Left" Margin="542,356,0,0" VerticalAlignment="Top" Height="43" Width="126" Background="#FF6D92AC" FontWeight="Bold" FontSize="18" BorderBrush="#FF050101" Click="btn_export_Click"/>

    </Grid>
</Window>
