@model IEnumerable<Homework_9.Models.Director>

@{
    ViewBag.Title = "Index";
}

<h2>All Directors</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Director_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Director_Name)
        </td>
        <td>
            
            @Html.ActionLink("Info", "Info", new { id=item.Director_Id }) 
           
        </td>
    </tr>
}

</table>
